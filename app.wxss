/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
  margin: 0rpx;
  background: #efefef;
}

.border-radius {
  border-radius: 15rpx;
}

.list {
  width: 100%;
  height: 100%;
  padding: 0rpx;
  margin: 0rpx;
  box-sizing: border-box;
  background: #efefef;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.list-item {
  height: 150rpx;
  width: 730rpx;
  margin-top: 15rpx;
  background: white;
  display: flex;
  flex-direction: row;
  border-radius: 15rpx;
  border: 1rpx solid #efefef;
  box-sizing: border-box;
}
